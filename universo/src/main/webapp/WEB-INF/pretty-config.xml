<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping>
		<pattern value="/contato" />
		<view-id value="/resources/pages/Cadastro/Contato/Contato.xhtml"/>
	</url-mapping>

	<url-mapping>
		<pattern value="/usuario" />
		<view-id value="/resources/pages/Cadastro/Usuario/Usuario.xhtml"/>
	</url-mapping>
	
	<url-mapping>
		<pattern value="/login" />
		<view-id value="/login.xhtml"/>
	</url-mapping>
	
	<url-mapping>
		<pattern value="/home" />
		<view-id value="/resources/pages/home.xhtml"/>
	</url-mapping>

</pretty-config>